<Application
    x:Class="Labb3Extra.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:Labb3Extra.View"
    xmlns:viewModel="clr-namespace:Labb3Extra.ViewModel">

    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModel:StartViewModel}">
            <view:StartView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModel:UserProfileViewModel}">
            <view:UserProfile />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModel:AdminViewModel}">
            <view:AdminView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModel:ShopViewModel}">
            <view:ShopView />
        </DataTemplate>
    </Application.Resources>
</Application>